{
  "name": "mdast-util-escape",
  "version": "1.0.0",
  "description": "Utility to escape text to be inserted into mdast",
  "license": "MIT",
  "keywords": [
    "mdast",
    "markdown",
    "escape",
    "text",
    "util",
    "utility"
  ],
  "repository": "wooorm/mdast-util-escape",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^11.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.3.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "mdast": "^2.0.0",
    "mdast-comment-config": "^1.0.0",
    "mdast-github": "^1.0.0",
    "mdast-lint": "^1.0.0",
    "mdast-slug": "^2.0.0",
    "mdast-validate-links": "^1.0.0",
    "tape": "^4.2.0"
  },
  "scripts": {
    "build-md": "mdast . --quiet --frail",
    "build-bundle": "browserify index.js --no-builtins -s mdastUtilEscape > mdast-util-escape.js",
    "build-mangle": "esmangle mdast-util-escape.js > mdast-util-escape.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
